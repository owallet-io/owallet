{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "nohoist": [
      "mobx-react-lite"
    ]
  },
  "scripts": {
    "clean": "rimraf ios/Podfile.lock",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15 Pro'",
    "release": "cd android && ./gradlew clean && cd ../ && npx react-native run-android --mode release",
    "release-ios": "react-native run-ios --mode Release --simulator='iPhone 15 Pro Max'",
    "fastlane-ios:release": "cd ios && bundle exec fastlane release",
    "fastlane-android:release": "cd android && bundle exec fastlane release",
    "fastlane-android:distribution": "cd android && ./gradlew clean && fastlane distribution",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "typecheck": "tsc",
    "watch:provider": "cross-env NODE_ENV=development webpack",
    "build:provider": "node ./src/scripts/create-sentry-properties.js && cross-env NODE_ENV=production webpack",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "pod:install": "pod-install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --detectOpenHandles",
    "postinstall": "patch-package && cd ../../ && rimraf node_modules/crypto && rimraf node_modules/@types/metro-config && yarn patch-package",
    "code-push-ios": "appcenter codepush release-react -a Oraichain/Owallet -t 3.1.1 --output-dir build",
    "code-push-android": "appcenter codepush release-react -a Oraichain/Owallet-1 -t 3.1.2 --output-dir build",
    "images": "node ./src/scripts/generate-images.js",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest ios"
  },
