  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "typecheck": "tsc",
    "watch:provider": "cross-env NODE_ENV=development webpack",
    "build:provider": "cross-env NODE_ENV=production webpack",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "generate:splash-image": "expo-splash-screen --background-color \"#141414\" --image-path ./src/assets/logo/splash-screen-only-k.png --image-resize-mode contain --status-bar-translucent --status-bar-background-color \"#141414\" --status-bar-style dark-content",
    "pod:install": "pod-install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --detectOpenHandles",
    "postinstall": "patch-package && cd ../../ && yarn patch-package",
    "code-push-ios": "appcenter codepush release-react -a Oraichain/Owallet -t 0.0.5 --output-dir build",
    "code-push-android": "appcenter codepush release-react -a Oraichain/Owallet-1 -t 1.0.1 --output-dir build"
