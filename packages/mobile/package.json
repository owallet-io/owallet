{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "nohoist": [
      "mobx-react-lite"
    ]
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "fastlane-ios:release": "cd ios && bundle exec fastlane release",
    "fastlane-android:release": "cd android && bundle exec fastlane release",
    "fastlane-android:distribution": "cd android && fastlane distribution",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "typecheck": "tsc",
    "watch:provider": "cross-env NODE_ENV=development webpack",
    "build:provider": "cross-env NODE_ENV=production webpack",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "pod:install": "pod-install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --detectOpenHandles",
    "postinstall": "patch-package && cd ../../ && rimraf node_modules/crypto && rimraf node_modules/@types/metro-config && yarn patch-package",
    "code-push-ios": "appcenter codepush release-react -a Oraichain/Owallet -t 0.0.31 --output-dir build",
    "code-push-android": "appcenter codepush release-react -a Oraichain/Owallet-1 -t 0.0.15 --output-dir build",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --sourcemap-output ios/main.jsbundle.map --assets-dest ios",
    "bundle:android": "react-native bundle --entry-file index.js --platform android --dev false --bundle-output android/index.android.bundle --sourcemap-output index.android.bundle.map --assets-dest android",
    "images": "node ./src/scripts/generate-images.js"
  },
